    "solid-js": "^0.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/history": "^4.7.3",
    "babel-preset-solid": "^0.18.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
